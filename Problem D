import java.util.Scanner;
public class App {
    public static void main(String[] args) {
        
        Scanner teman = new Scanner(System.in);
        int N = teman.nextInt();
        int M = teman.nextInt();

        int hasil = N / M;
        int sisa = N % M;


        System.out.println("masing-masing " + hasil);
        System.out.println("bersisa " + sisa);

    }
}
